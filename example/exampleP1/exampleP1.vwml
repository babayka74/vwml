module exampleP1 {
	Example1 ias (
		OnShelf ias nil;
		Robby ias (
			FigureSet ias (Cube Ball);
		        Creation ias (
				OnShelfSituation [
					(OnShelf FigureSet~ Random)^
				OnShelfSituation ]
			);
			
			source lifeterm = Creation~ Exe
		);
		Bob ias (
			ColorSet ias (Red Green);
			Attempt ias (
				OnShelfSituation [
					(Empty (Empty (OnShelf~ nil)Ident))^
				OnShelfSituation ]
				Empty~~ Exe
			);
			(Empty true) ias Attempt~ Exe;
			(Empty false) ias (OnShelf (OnShelf~ ColorSet~ Random))^;
			
			source lifeterm = Attempt~ Exe
		);
	);
}
